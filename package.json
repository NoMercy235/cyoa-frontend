{
  "name": "cyoa-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@nomercy235/utils": "^1.3.1",
    "axios": "^0.18.0",
    "axios-progress-bar": "^1.2.0",
    "classnames": "^2.2.6",
    "formik": "^1.4.1",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-utils": "^5.1.0",
    "moment": "^2.23.0",
    "mui-datatables": "^2.0.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-select": "^2.4.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start-https": "HTTPS=true react-app-rewired start",

    "start-http-prod": "npm run build-sw && npx http-server-spa build /index.html 3000",
    "start-http-prod-no-build": "npm run build-sw && npx http-server-spa build /index.html 3000",
    "http-server": "npx http-server-spa build /index.html 3000",

    "start-https-prod": "npm run build && npm run https-server",
    "https-server": "PORT=3000 npx https-localhost build/",

    "build": "react-app-rewired build && npm run build-sw && npm run clean-cra-sw",
    "build-sw": "node ./src/swBuild.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",

    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "serve": "npm run docker-setup && npm run clean-node-modules",
    "docker-setup": "docker-compose down && npm i && npm run build && docker-compose build --no-cache && docker-compose up -d",
    "clean-node-modules": "rm -rf node_modules/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "react-app-rewired": "^1.6.2",
    "workbox-build": "^4.3.1"
  }
}
